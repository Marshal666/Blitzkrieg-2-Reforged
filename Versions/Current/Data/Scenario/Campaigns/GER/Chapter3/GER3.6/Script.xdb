function LooseCheck()
	while 1 do
        if ( IsSomePlayerUnit( 0 ) == 0 ) and ( GetReinforcementCallsLeft ( 0 ) &lt;= 0 ) then
            Wait(3);
			Trace(&quot;Defeat&quot;);
			Win( 1 );
			return 1;
		end	
		Wait( 2 );
	end;
end;
