-------------------------------------------------Art_Action
function ArtAction()
	Wait ( 5 );
	local k=GetObjectList ( 701 );
	local c=GetObjectList ( 702 );
	Wait ( 2 );
	while 1 do 
		if ( UnitCanSee ( k, 400, 0 ) == 1 ) or ( UnitCanSee ( c, 400, 0 ) == 1 ) then
			Wait ( 1 );
			Cmd( ACT_SUPPRESS, 160, 500, 4595, 6966 );
			Trace (&quot;Art_Action&quot;)
			Wait (5);
		return 1;
		end;
		Wait (3);
	end;
end;

-----------------------------------------------Bomber Strike
function Bomb_Strike()
	local k=0;
	while k&lt;=5 do
		if ( IsSomeBodyAlive ( 1, 200 ) == 0 ) or ( IsSomeBodyAlive ( 1, 201 ) == 0 ) then
			Wait ( 3 );
			LandReinforcementFromMap (1, &quot;bomber&quot;, 0, 1001 );
			Wait ( 3 );
			Cmd (ACT_MOVE, 1001, 500, 7232, 1200 );
			Trace(&quot;Bombimg_srike&quot;);
			k=k+1;
			Wait ( 90 );
		end;
		Wait ( 2 );
	end;
end;
