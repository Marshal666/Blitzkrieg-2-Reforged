	end;
end;

------------------------------------------------------Victory
function WinCheck()
	local win = 0	
	while ( win == 0 ) do
		Wait( 3 );
		if ( GetNUnitsInScriptGroup( 400 ) &lt;= 1 ) then
			win = 1;
			Wait( 3 );
			Trace( &quot;mission get win EnemyInCity=%g&quot;, GetNUnitsInArea( 1, &quot;IRCR&quot;, 0 ));
			CompleteObjective ( 1 );
			Wait ( 2 );
			GiveObjective ( 0 );
			StartThread (WinCheckComplete);
		end;
	end;
end;
------------------------------------------------------VictoryComplete
function WinCheckComplete()
	local win2 = 0
	while ( win2 == 0 ) do
		Wait( 3 );
		if ( GetNUnitsInArea( 0, &quot;IRCR&quot;, 0 ) &gt;= 1 ) and ( GetNUnitsInArea( 1, &quot;IRCR&quot;, 0 ) &lt;= 0 ) then
			win2 = 1;
			Wait( 3 );
			Trace( &quot;mission win complete EnemyInIRCR=%g&quot;, GetNUnitsInArea( 1, &quot;IRCR&quot;, 0 ));
			CompleteObjective ( 0 );
			Wait ( 1 );
			Win ( 0 );
			return 1;
		end;
		Wait (1);
	end;
end;
