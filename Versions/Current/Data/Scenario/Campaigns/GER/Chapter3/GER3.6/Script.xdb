------------------------------------------------------CAttack
function Wawe2()
	while 1 do
        if ( GetNUnitsInArea( 0, &quot;AZ1&quot;, 0 ) &gt;= 0 ) or ( GetNUnitsInArea( 0, &quot;AZ3&quot;, 0 ) &gt;= 0 )then
            Wait ( 2 );
			StartThread( CA_GO );
			Wait ( 2 );
			StartThread( CA_1_GO );
		end	
		Wait( 2 );
	end;
end;

------------------------------------------------------CAttack2
function Wawe3()
