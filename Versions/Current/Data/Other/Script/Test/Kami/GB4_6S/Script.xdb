----------///Win//------------
function Winner()
	while 1 do
		Wait ( 3 )
		if ((GetNUnitsInScriptGroup(7000, 1) &lt;= 0) and (GetNUnitsInScriptGroup(8000, 1) &lt;= 0) and (GetNUnitsInScriptGroup(9000, 1) &lt;=0) and (GetNUnitsInScriptGroup(10000, 1) &lt;= 0) ) then
		Wait(3)
		CompleteObjective(0)
		Wait ( 5 )
		Win ( 0 )
		break
		end;
		Wait(1)
	end;
	Wait(1)
end;

-------------//Loose//-----------------

function Looser()
	while 1 do
		Wait ( 3 )
		if ((GetNUnitsInScriptGroup(7000, 1) &gt; 1) and (GetNUnitsInScriptGroup(8000, 1) &gt; 1) and (GetNUnitsInParty(0) &lt;= 0) and (GetReinforcementCallsLeft(0)) &lt;= 0) then
		Wait ( 5 )
		Win ( 1 )
		break
		end;
		Wait(1)
	end;	
end;

function Ka_boom()
	while 1 do
		Wait(3)
		if(GetNUnitsInArea(1, &quot;DPoint&quot;) &gt;=1) then
		Wait ( 5 )
		Loose(0)
		break
		end;
		Wait(3)
	end;
	Wait(1)
end;

