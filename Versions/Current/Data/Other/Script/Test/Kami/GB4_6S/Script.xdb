end;


function Ent()
	Wait(5)
	Cmd(ACT_ENTRENCH, 9000, 1)
end;

--------------//German_Recon//----------------

function GerRecon() 
	while 1 do
		Wait ( 3 )	
		if (GetNUnitsInArea(0, &quot;Point&quot;, 0) &gt;= 1) then
			Wait ( 5 )
			GetNUnitsInScriptGroup( 5000 )
			Wait ( 5 )
			Cmd(ACT_SWARM, 5000, 0, 1812, 2274)
		break
		end;
		Wait(1)
	end;	
end;


