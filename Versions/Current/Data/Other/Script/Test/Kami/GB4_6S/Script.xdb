function Re()
	while 1 do
		Wait ( 3 )
		if((GetNUnitsInScriptGroup(7000, 1) &lt;= 0) and (GetNUnitsInScriptGroup(8000, 1) &lt;= 0)) then
		Wait(3)
		LandReinforcement(1, 680, 0, 9000)
		Wait(1)
		Cmd(ACT_MOVE, 9000, 2095, 4960)
		Wait(1)
		QCmd(ACT_MOVE, 9000, 3277, 5087)
		Wait(1)
		QCmd(ACT_MOVE, 9000, 4019, 4051)
		Wait(1)
		QCmd(ACT_MOVE, 9000, 5050, 3053)
		Wait(1)
		QCmd(ACT_MOVE, 9000, 5880, 1995)
		Wait(1)
		QCmd(ACT_MOVE, 9000, 6016, 772)
		break
		end;
	end;
	Wait(1)
end;

