	StartThread( CompleteObjective0 );
end;

function CompleteObjective0()
	while 1 do
		Wait( 3 );
		if IsSomeUnitInArea ( 1 , &quot;hqzone&quot; , 0 ) &gt; 0 and IsSomeUnitInArea ( 0 , &quot;hqzone&quot; , 0 ) &lt; 1 then
			Win ( 1 );
			break;
		end;	
	end;
end;

function Objective1()
