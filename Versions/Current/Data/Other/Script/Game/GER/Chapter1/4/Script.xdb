function Objective()	ObjectiveChanged(0, 1); 	SetIGlobalVar( &quot;temp.objective0&quot;, 1 );	StartThread( CompleteObjective0 );	Wait( 1 );end;
function CompleteObjective0()	while 1 do		Wait( 1 );		if (GetNUnitsInArea ( 1 , &quot;Town&quot; , 0 ) &lt; 1) then			StartThread( Winner );			Wait ( 1 );			break;		end;	end;end;
------------------------WIn_Loose
function Winner()	while 1 do		Wait( 1 );		if (GetNUnitsInArea ( 0 , &quot;Town&quot; , 0 ) &gt; 0) and (wol == 0 ) then			wol = 1;
			ObjectiveChanged(0, 2);			SetIGlobalVar( &quot;temp.objective0&quot;, 2 );			Wait( 1 );			Win(0);			break;		end;		if (GetNUnitsInArea ( 2 , &quot;Town&quot; , 0 ) &gt; 0) and (wol == 0) then			wol = 1;			Wait( 1 );			Win(1);			break;		end;	end;end;
function Unlucky()	while 1 do		Wait( 3 );        if ( GetNUnitsInParty( 0 ) &lt; 1) and ( GetReinforcementCallsLeft( 0 ) == 0 ) then			Wait( 1 );			Win(1);			break;		end;	end;end;
