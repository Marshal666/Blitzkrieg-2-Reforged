global proc setAllKey(string $g)
{
  eval ("setAttr -k on "+$g+".StartTime");
  eval ("setAttr -k on "+$g+".EndTime");
  eval ("setAttr -k on "+$g+".PISTOL");
  eval ("setAttr -k on "+$g+".MACHINEGUN");
  eval ("setAttr -k on "+$g+".SUBMACHINEGUN");
  eval ("setAttr -k on "+$g+".RIFLE");
  eval ("setAttr -k on "+$g+".SNIPER_RIFLE");
  eval ("setAttr -k on "+$g+".ANTITANK_RIFLE");
  eval ("setAttr -k on "+$g+".BAZOOKA");
  eval ("setAttr -k on "+$g+".looped");

}

global proc addAPsel()
{
 $sel = `ls -sl`;
 if (size($sel)==0) print "\nError! Select locator!";
 else
          {
               addAttr -ln action_point -at long  -min 0 -max 1000 -dv 0 -k on $sel[0];
                eval ("setAttr -k on "+$sel[0]+".action_point");

          }
}


