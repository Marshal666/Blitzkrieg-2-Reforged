////////////////////////////////////////////////PROCEDURES/////////////////////////////////////////

//--------------------------------Creation Locators

global proc createAnLocator(string $v)
{
 $fieldName1_ = $v  +"_sf";
 $fieldName2_ = $v  +"_ef";


 spaceLocator -p 0 0 0 -n $v;
 select $v;
 //Adding start&end attr.
 addAttr -ln StartTime -sn sf  -at long  -min 0 -max 1000 -dv 1 -k on $v;
 addAttr -ln EndTime        -at long  -min 0 -max 1000 -dv 1 -k on $v;
 //Adding weapon attr.
 addAttr -ln PISTOL           -at bool  -min 0 -max 1 -dv 0 -k on $v;
 addAttr -ln MACHINEGUN       -at bool  -min 0 -max 1 -dv 0 -k on $v;
 addAttr -ln SUBMACHINEGUN    -at bool  -min 0 -max 1 -dv 0 -k on $v;
 addAttr -ln RIFLE            -at bool  -min 0 -max 1 -dv 0 -k on $v;
 addAttr -ln SNIPER_RIFLE     -at bool  -min 0 -max 1 -dv 0 -k on $v;
 addAttr -ln ANTITANK_RIFLE   -at bool  -min 0 -max 1 -dv 0 -k on $v;
 addAttr -ln BAZOOKA          -at bool  -min 0 -max 1 -dv 0 -k on $v;
 addAttr -ln looped           -at bool  -min 0 -max 1 -dv 0 -k on $v;


 connectControl $fieldName1_ ($v+".StartTime");
 connectControl $fieldName2_ ($v+".EndTime");

 parent $v Animations;


}




//----------------------------------Feedback from Changing check box anim
global proc change_chb(string $z)
{
 //set names
 $ch_boxName_ = "chb_"+$z;
 $butName_    = "b_"  +$z;
 $fieldName1_ = $z  +"_sf";
 $fieldName2_ = $z  +"_ef";

 //get value of box
 $x = `checkBox -q -v $ch_boxName_`;
 print $x;

 //creation anim. locator
 if ($x) createAnLocator($z);
 else
     {
      intField -e -v 0 $fieldName1_;
      intField -e -v 0 $fieldName2_;
      delete $z;
     }

 //set fields conditions
 button -e -en $x $butName_;
 intField -e -en $x $fieldName1_;
 intField -e -en $x $fieldName2_;
}


global proc int exist(string $s)
{
 $list = `ls $s`;
 if (size($list)==0) return 0;
 else return 1;
}



