function Train()
	while 1 do
		if GetNUnitsInArea(1, &quot;Village1&quot;, 0) &gt; 0 then
			Cmd(0, 900, 50, GetScriptAreaParams (&quot;T1&quot;));
		end;
		if GetNUnitsInArea(1, &quot;Village2&quot;, 0) &gt; 0 then
			Cmd(0, 900, 50, GetScriptAreaParams (&quot;T2&quot;));
		end;
		Wait(5);
	end;
end;
function Reinf()
	while 1 do
		for k = 1, 6 do
			if GetNUnitsInScriptGroup(k, 3 ) &lt; 1 then
				LandReinforcementFromMap ( 3, &quot;Vzvod&quot;, 0, k );
				Wait(1);
				ke = k+10;
				Cmd( ACT_SWARM, k, 50, GetScriptAreaParams (&quot;P2&quot;));	
				QCmd( ACT_ENTER, k, ke);		
			end;
		end;
		Wait(3);
	end;
end;
function Reinf1()
	while 1 do
		for k1 = 7, 10 do
			if GetNUnitsInScriptGroup(k1, 3 ) &lt; 1 then
				LandReinforcementFromMap ( 3, &quot;Vzvod&quot;, 1, k1 );
				Wait(1);
				ke1 = k1+10;
				Cmd( ACT_SWARM, k1, 50, GetScriptAreaParams (&quot;P1&quot;));	
				QCmd( ACT_ENTER, k1, ke1);		
			end;
		end;
		Wait(3);
	end;
end;
