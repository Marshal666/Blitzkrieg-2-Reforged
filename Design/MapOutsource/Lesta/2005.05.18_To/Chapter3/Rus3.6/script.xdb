	<ScriptText>-- Lake
function Loose()    while 1 do        if ( GetNUnitsInParty(0) &lt; 1 and GetNUnitsInParty(3) &lt; 1 and GetReinforcementCallsLeft( 0 ) == 0 ) then			Win(1);			return 1;		end;	Wait(5);	end;end;
function Loose1()    while 1 do		if (GetNUnitsInArea(1, &quot;Village1&quot;, 0) &gt; 0 and GetNUnitsInArea(0, &quot;Village1&quot;, 0) &lt; 1 and GetNUnitsInArea(3, &quot;Village1&quot;, 0) &lt; 1) then			FailObjective (0);
			Wait(1);			Win(1);			return 1;		end;	Wait(5);	end;end;
function Loose2()    while 1 do		if (GetNUnitsInArea(1, &quot;Village2&quot;, 0) &gt; 0 and GetNUnitsInArea(0, &quot;Village2&quot;, 0) &lt; 1 and GetNUnitsInArea(3, &quot;Village2&quot;, 0) &lt; 1) then			FailObjective (0);
			Wait(1);			Win(1);			return 1;		end;	Wait(5);	end;end;
function Attack()
	for c = 1, 3 do		a = Random(2);
		b = Random(2);
		LandReinforcementFromMap ( 1, &quot;Attack1&quot;, 0, 100 );		Wait(2);		Cmd( ACT_SWARM, 100, 50, GetScriptAreaParams (&quot;A1&quot; .. a));		QCmd( ACT_SWARM, 100, 50, GetScriptAreaParams (&quot;Village1&quot;));
		Wait(15);
		LandReinforcementFromMap ( 1, &quot;Attack2&quot;, 0, 103 );		Wait(2);		Cmd( ACT_SWARM, 103, 50, GetScriptAreaParams (&quot;A1&quot; .. a));		QCmd( ACT_SWARM, 103, 50, GetScriptAreaParams (&quot;Village1&quot;));
		Wait(100);
		LandReinforcementFromMap ( 1, &quot;Attack1&quot;, 1, 101 );		Wait(2);		Cmd( ACT_SWARM, 101, 50, GetScriptAreaParams (&quot;A2&quot; .. b));		QCmd( ACT_SWARM, 101, 50, GetScriptAreaParams (&quot;Village2&quot;));		
		Wait(15)		LandReinforcementFromMap ( 1, &quot;Attack2&quot;, 1, 104 );		Wait(2);		Cmd( ACT_SWARM, 104, 50, GetScriptAreaParams (&quot;A2&quot; .. b));		QCmd( ACT_SWARM, 104, 50, GetScriptAreaParams (&quot;Village2&quot;));
		Wait(180+Random(100))
	end;
		StartThread( TheLastBattle);end;
function TheLastBattle()
		a = Random(2);
		b = Random(2);
		LandReinforcementFromMap ( 1, &quot;Attack11&quot;, 0, 200 );		Wait(2);		Cmd( ACT_SWARM, 200, 50, GetScriptAreaParams (&quot;A1&quot; .. a));		QCmd( ACT_SWARM, 200, 50, GetScriptAreaParams (&quot;Village1&quot;));
		LandReinforcementFromMap ( 1, &quot;Attack11&quot;, 1, 201 );		Wait(2);		Cmd( ACT_SWARM, 201, 50, GetScriptAreaParams (&quot;A2&quot; .. b));		QCmd( ACT_SWARM, 201, 50, GetScriptAreaParams (&quot;Village2&quot;));
		StartThread( Winner);		
		Wait(180);
		Win(0);
end;
function Winner()
	while 1 do
		if GetNUnitsInScriptGroup( 200, 1 ) &lt; 1 and GetNUnitsInScriptGroup( 201, 1 ) &lt; 1 then
		Win(0);
		end;
		Wait(3);
	end;
end;
