Wait(1);
GiveObjective( 0 );
Wait( 2 )
GiveObjective( 1 );
StartThread( Loose);
StartThread( Loose1);
StartThread( Loose2);
StartThread( Train);
StartThread( Reinf);
StartThread( Reinf1);
Wait(10);
StartThread( Attack);
